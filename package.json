{
  "name": "jobmanager",
  "version": "1.0.0",
  "description": "Ein digitales Bewerbungs-Management-Tool f√ºr Berufseinsteiger",
  "main": "dist/main/main.js",
  "homepage": "https://github.com/Sykios/JobManager",
  "author": {
    "name": "Sykios"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:electron\"",
    "dev:renderer": "webpack --config webpack.renderer.dev.config.js --watch",
    "dev:main": "webpack --config webpack.main.dev.config.js --watch",
    "dev:electron": "wait-on dist/main/main.js dist/renderer/index.html && cross-env NODE_ENV=development electron .",
    "dev:quick": "npm run build:dev && npm run dev:electron",
    "build:dev": "webpack --config webpack.renderer.dev.config.js && webpack --config webpack.main.dev.config.js",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "start": "electron .",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:win-unsigned": "set CSC_IDENTITY_AUTO_DISCOVERY=false && npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "publish": "npm run build && electron-builder --publish=always",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:sync": "node tests/test-sync.js",
    "create-test-data": "node tests/create-test-data.js",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.0",
    "axios": "^1.11.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "dotenv": "^17.2.2",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "sqlite": "^4.2.1",
    "sqlite3": "^5.1.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/sqlite3": "^3.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "buffer": "^6.0.3",
    "concurrently": "^7.6.0",
    "cross-env": "^10.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.7.3",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "esbuild-loader": "^4.3.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "build": {
    "appId": "com.sykios.jobmanager",
    "productName": "JobManager",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "protocols": [
      {
        "name": "jobmanager-protocol",
        "schemes": [
          "jobmanager"
        ]
      }
    ],
    "publish": {
      "provider": "github",
      "owner": "Sykios",
      "repo": "JobManager",
      "publishAutoUpdate": true,
      "releaseType": "prerelease"
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "forceCodeSigning": false
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "keywords": [
    "bewerbungsmanager",
    "job-application",
    "electron",
    "react",
    "typescript",
    "desktop-app"
  ]
}
